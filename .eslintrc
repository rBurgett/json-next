{
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "extends": "eslint:recommended",
    "rules": {
        "quotes": [1, "single", "avoid-escape"],
        "semi": 2,
        "no-console": 0,
        "no-alert": 2
    },
    "plugins": [],
    "env": {
        "node": true,
        "es6": true
    },
    "globals": {}
}
